<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateFindToolWindow">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="f" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="Editor Cut">
    <keyboard-shortcut first-keystroke="ctrl x" />
  </action>
  <action id="EditorSelectWord" />
  <action id="Find">
    <keyboard-shortcut first-keystroke="ctrl f" second-keystroke="f" />
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="ctrl f" second-keystroke="ctrl f" />
  </action>
  <action id="FindModal">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="f" />
  </action>
  <action id="Images.Thumbnails.Hide" />
  <action id="Replace">
    <keyboard-shortcut first-keystroke="ctrl f" second-keystroke="r" />
  </action>
  <action id="ReplaceInPath">
    <keyboard-shortcut first-keystroke="ctrl f" second-keystroke="ctrl r" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="Vcs.QuickListPopupAction">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="v" />
  </action>
</keymap>